Component EqArrs {
  data lhs : int[][];
  data rhs : int[][];

  relation Eq(x: int[][], y: int[][]) {
    foreach (p, q) in (x, y) {
      foreach (s, t) in (p, q) {
        s = t
      }
    }
  }


  
  always Eq(lhs, rhs);
}


Component Main {
  
  data x : int[][];
  data y : int[][];
  data z : int[][];

