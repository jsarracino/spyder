Component Bound {
  data x: int;
  data y: int;
  data z: int;

  relation solveX(x: int) {
    y = x + 1 && z = x + 2
  }
  relation solveY(y: int) {
    x = y - 1 && z = y + 1
  }
  relation solveZ(z: int) {
    y = z - 1 && x = z - 2
  }
  
}

