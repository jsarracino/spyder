Component Main {
  
  data xs: int;
  data ys: int;
  data zs: int;
  

  procedure incX() {
    for (x: int, y: int, z: int) in (xs, ys, zs) {
      x = x + 1;
      y = y + 1;
      z = z + 1;
    }
  }

  procedure decY() {
    for (y: int, x: int) in (ys, xs) {
      y = y - 1;
      x = x - 1;
    }
  }

  procedure incY() {
    for (y: int, z: int) in (ys, zs) {
      y = y + 1;
      z = z + 1;
    }
  }

  procedure decZ() {
    for (z: int, y: int, x: int) in (zs, ys, xs) {
      z = z - 1;
      y = y - 1;
      x = x - 1;
    }
  }

  procedure incZ() {
    for (z: int) in (zs) {
      z = z + 1;
    }
  }

  
}