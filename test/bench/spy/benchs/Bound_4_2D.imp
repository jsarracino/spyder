Component Main {
  
  data xss: int;
  data yss: int;
  data zss: int;
  data uss: int;


  procedure incX() {
    for (xs: int[], ys: int[], zs: int[], us: int[]) in (xss, yss, zss, uss) {
      for (x: int, y: int, z: int, u: int) in (xs, ys, zs, us) {
        x = x + 1;
        y = y + 1;
        z = z + 1;
        u = u + 1;
      }
    }
  }

  procedure decY() {
    for (ys: int[], xs: int[]) in (yss, xss) {
      for (y: int, x: int) in (ys, xs) {
        y = y - 1;
        x = x - 1;
      }
    }
  }

  procedure incY() {
    for (ys: int[], zs: int[], us: int[]) in (yss, zss, uss) {
      for (y: int, z: int, u: int) in (ys, zs, us) {
        y = y + 1;
        z = z + 1;
        u = u + 1;
      }
    }
  }

  procedure decZ() {
    for (zs: int[], ys: int[], xs: int[]) in (zss, yss, xss) {
      for (z: int, y: int, x: int) in (zs, ys, xs) {
        z = z - 1;
        y = y - 1;
        x = x - 1;
      }
    }
  }

  procedure incZ() {
    for (zs: int[], us: int[]) in (zss, uss) {
      for (z: int, u: int) in (zs, us) {
        z = z + 1;
        u = u + 1;
      }
    }
  }
  procedure decU() {
    for (us: int[], zs: int[], y: int[], x: int[]) in (uss, zss, yss, xss) {
      for (u: int, z: int, y: int, x: int) in (us, zs, ys, xs) {
        u = u - 1;
        z = z - 1;
        y = y - 1;
        x = x - 1;
      }
    }
  }

  
}