Component Shared {
  data l: int;
  data r: int;
  data m: int;

  relation Mid(l: int, r: int, m: int) {
    m = (l + r) / 2
  }

  always Mid(l, r, m);
  
}

Component Main {
  data me: int;
  data them: int;
  data mid: int;

  using Shared(me, them, mid);

  procedure addRowMeThem() {
    let val: int;
    let val2: int;
    me = val;
    them = val2;
  }

  
  procedure addRowMe() {
    let val: int;
    me = val;
  }
  


}