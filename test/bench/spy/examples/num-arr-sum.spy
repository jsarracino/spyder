Component SumArr {
  data sum : int[];
  data xs : int[];

  relation Sum(ss: int[], xs: int[]) {
    foreach (s, b) in (ss, xs) {
      s = prev(s, 0) + b
    }
  }


  
  always Sum(sum, xs);
}


