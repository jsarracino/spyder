Component Sum2Ten {
  data lhs : int;
  data rhs: int;

  relation toTen(x: int, y: int) {
    x + y == 10
  }
  
  always toTen(lhs, rhs);
  
}


Component Main {
  
  data x: int;
  data y: int;

  using Sum2Ten(x, y);
  
  
  procedure init(inp: int) {
    if (inp == 0) {
      x = x + 1;
    } else {
      y = y + 1;
    }
  }


  
}